{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "globals": {
        "IEvent": "readonly"
    },
    "extends": [
        "standard"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "indent": ["error", 4]
    }
}
